	'002'																	, ; //X7_SEQUENC
	"POSICIONE('QAA',1,xFilial('QAA')+M->QI2_MATRES,'QAA_CC')"				, ; //X7_REGRA
	'QI2_DESDEP'															, ; //X7_CDOMIN
