	''																		, ; //X7_ALIAS
	0																		, ; //X7_ORDEM
	''																		, ; //X7_CHAVE
	'U'																		, ; //X7_PROPRI
	''																		} ) //X7_CONDIC

//
// Campo QI2_CODORI
//
aAdd( aSX7, { ;
	'QI2_CODORI'															, ; //X7_CAMPO
	'002'																	, ; //X7_SEQUENC
	"''"																	, ; //X7_REGRA
